<UserControl x:Class="TheoryOfInformation.lab2.TextWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TheoryOfInformation.lab2"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="outputText" TextWrapping="Wrap" PreviewTextInput="keyBox_PreviewTextInput" Margin="4" FontSize="16" Text="Вывод"/>
        <TextBox x:Name="outputText2" TextWrapping="Wrap" Margin="4" FontSize="16" Text="Вывод" Grid.Row="1"/>
    </Grid>
</UserControl>
